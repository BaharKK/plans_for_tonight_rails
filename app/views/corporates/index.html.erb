<h1>Corporates#index</h1>
<p>Find me in app/views/corporates/index.html.erb</p>
<%= link_to "New Event", new_event_path %>
<p> Current Events </p>
<% current_user.events.each do |event| %>
  <h2><%= event.name %> (<%= link_to "edit", edit_event_path(event) %>, <%= link_to "delete", event_path(event), method: :delete,confirm: "You sure?" %>) | <%= event.location %> </h2>
  <%= image_tag event.image_url(:thumb) if event.image? %>
  <p>Event Title: <%= link_to event.name, event_path(event.id) %></p>
  <p>Description: <%= event.description %></p>
  <p> Show Date: <%= event.show_date %> | Start Time <%= event.start_time %></p>
  <p> Duration <%= event.duration %> </p>
<% end %>
<p> Past Events </p>